 #include <iostream>
using namespace std;

template <typename T, int n>
class Point {
public:
    Point() {
        for (int i = 0; i < n; ++i) {
            data[i] = T();
        }
    }

private:
    T data[n];
};
int main() {
    Point<int, 3> intPoint;
    
    Point<double, 2> doublePoint;

    return 0;
}
