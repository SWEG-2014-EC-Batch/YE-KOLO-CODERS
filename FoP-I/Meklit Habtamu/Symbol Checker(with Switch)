#include <iostream>

using namespace std;

int main()
{
  char n;
    cout<<"Enter character:";
    cin>>n;
    if (n>='0'&&n<='9')
{
    cout<<" You have entered a number";
    switch (n%2)
    {
   case 0:
   cout<< n<<"is even";
   break;
   default:
   cout<< n<<"is odd";
   break;
    }
}

    else if (n<='A'&&n>='Z')
{

    cout<< "You have entered a capital letter";
}
else
{
    cout<< "You have entered a special character";
}
    return 0;
}
